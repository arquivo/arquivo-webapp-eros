<%
  const year = requestedPage.timestamp.slice(0, 4);
  const month = requestedPage.timestamp.slice(4, 6);
  const day = requestedPage.timestamp.slice(6, 8);
  const hours = requestedPage.timestamp.slice(8, 10);
  const minutes = requestedPage.timestamp.slice(10, 12);
  const seconds = requestedPage.timestamp.slice(12, 14);
  const date = t('common.date.long',{
    year: year,
    month: t('common.months.'+month),
    day: parseInt(day),
    hours: hours,
    minutes: minutes
  });
%>
<!-- starts right nav menu -->
<nav id="replay-right-nav">
<h3><%= t('wayback.options-menu.accessibility'); %></h3> 
<button id="right-nav-close-button" onclick="closeReplayRightMenuNav()"><span>&times;</span></button>
<section>
  <h4><%= t('wayback.options-menu.accessibility-list'); %></h4>
    <ul>      
      <li id="menuUrl" class="link-line-height"><span><%- requestedPage.url %></span><br><span id="menuTs" class="page-date"><%- date %></span></li>
      <li><a id="menuListVersions" href="/page/search?q=<%- requestedPage.url %>"><span class="fas fa-list" aria-hidden="true"></span><%= t('wayback.options-menu.list-versions'); %></a></li>
      <li><a id="menuTechnicalDetails" href="#technical-details" rel="modal:open"><span class="fas fa-info-circle" aria-hidden="true"></span><%= t('wayback.options-menu.technical-details'); %></a></li>
      <li><a id="menuScreenshot" href="#screenshot" rel="modal:open""><span class="fas fa-camera" aria-hidden="true"></span><%= t('wayback.options-menu.screenshot'); %></a></li>
      <li><a id="menuPrint" href="#print" rel="modal:open"><span class="fas fa-print" aria-hidden="true"></span><%= t('wayback.options-menu.print'); %></a></li>
      <li><a id="menuCompleteThePage" href="#complete-the-page" rel="modal:open"><span class="fas fa-laptop-medical" aria-hidden="true"></span><%= t('wayback.options-menu.complete-page'); %></a></li>
      <li><a id="menuFullScreen" href="<%- config.get('pywb.url') + '/' +requestedPage.fullUrl %>" ><span class="fas fa-expand-arrows-alt" aria-hidden="true"></span><%= t('wayback.options-menu.fullscreen'); %></a></li>
      <li><a id="menuReplayWithOldBrowser" href="#replay-with-old-browser" rel="modal:open"><span class="fab fa-internet-explorer ie" aria-hidden="true"></span><%= t('wayback.options-menu.replay-with-old-browser'); %></a></li>
    </ul>
</section>
</nav>
<!-- ends right nav menu -->