<!-- starts datepicker left calendar -->
<script>  
	$(function () {	
		
	  $(".call-datepicker-start-year").datepicker({
		
		dateFormat:"yy dd M",		
		changeYear:true,
		changeMonth:true,
		  onSelect: function(dateCalendarLeft) {
			var calendarActulDate = new Date("yy dd M");
			$(('#end-year').substring(5));
			var x = document.getElementById("calendar-day-month-start").value;
	        if(x != "") {	  		  
			$('#calendar-day-month-start').val(dateCalendarLeft.substring(5));
	        }	
	        else if(x == "") {
		    $('#calendar-day-month-start').val(calendarActulDate);			
	   } 
	 }
   });
 });		
</script>
<!-- ends datepicker left calendar -->

<!-- starts datepicker right calendar -->
