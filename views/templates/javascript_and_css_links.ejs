<!-- starts jquery slider -->
<script>
  $( function() {
    $( "#slider-range" ).slider({
      range: true,
      min: 1996,
      max: 2021,
      values: [ 1996, 2021 ],
      slide: function( event, ui ) {
        $( "#start-year" ).val( ui.values[ 0 ]);
        $( "#end-year" ).val( ui.values[ 1 ]);
      }      
    });
    $( "#start-year" ).val($("#slider-range" ).slider( "values", 0 ));
     $( "#end-year" ).val($("#slider-range" ).slider( "values", 1 ));
  });
  </script>
<!-- ends jquery slider --> 

