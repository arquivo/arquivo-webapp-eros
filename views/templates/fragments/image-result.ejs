    <form style="display: none;" id="search-result-form-<%= imageData.index %>" action="/image/view/<%- requestData.get('trackingId') %>_<%= imageData.index %>/<%= imageData.imgTstamp %>/<%= encodeURIComponent(imageData.imgSrc) %>"></form>
    <ul>
