<!doctype html>
<html lang="<%- getLanguage() %>">

    <head>
    <!-- starts global document: <head>...</head> -->
    <%- include("../templates/head",{pagename:'pages',meta:{ query: requestData.get('q')} }) -%>
    <!-- ends global document: <head>...</head> -->
    
    <!-- starts styling for search results -->
    <link rel="stylesheet" type="text/css" href="/css/styles-search-results.css" media="screen">
    <!-- ends styling for search results -->

    <!-- starts logic for exporting -->
    <script src="/js/exports.js"></script>
    <!-- ends logic for exporting -->

    <!-- starts logic for text selection on search results -->
    <script src="/js/page-search-results.js"></script>
    <!-- ends logic for text selection on search results -->
    </head>

    <!-- starts document body content -->
    <body id="pages-search-results" class="pages-search-results-cl">  

            <!-- starts skip to main content -->
            <%- include("../templates/fragments/skip-to-main-content") -%>
            <!-- ends skip to main content -->

            <!-- adds headers for search engines -->
            <%- include("../templates/header/header-search-engine") -%>
            <!-- ends headers for search engines -->        
             
            <!-- starts include header pages search results -->     
            <%- include("../templates/header/header-pages-search-results") -%>
            <!-- ends include header pages search results --> 

            <!-- starts main container--> 
            <main id="main-content">      

                <!-- starts include body pages search results -->     
                <%- include("../templates/body/body-pages-search-results") -%>
                <!-- ends include body pages search results -->    

            </main>
            <!-- ends main container-->
            
            <!-- starts footer -->
            <%- include("../templates/footer") -%>
            <!-- ends footer --> 
            
    </body>
    <!-- ends document body content -->

</html>
